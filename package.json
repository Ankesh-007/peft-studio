{
  "name": "peft-studio",
  "version": "1.0.1",
  "description": "Desktop application for Parameter-Efficient Fine-Tuning of Large Language Models",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:no-check": "vite build",
    "build:analyze": "npm run build && open dist/stats.html",
    "build:electron": "npm run build && electron-builder --dir",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "test:scripts": "vitest --run --config scripts/vitest.config.js",
    "test:scripts:pbt": "vitest --run --config scripts/vitest.config.js scripts/test/*.pbt.test.js",
    "test:bundle": "vitest --run src/test/bundle-size-constraint.test.ts",
    "test:bundle-size": "vitest --run src/test/bundle-size-constraint.test.ts",
    "test:performance": "vitest --run src/test/performance.test.ts",
    "test:e2e": "echo 'E2E tests not yet implemented'",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "type-check": "tsc --noEmit",
    "electron:dev": "electron .",
    "electron:build": "electron-builder",
    "package": "npm run build && electron-builder",
    "package:win": "npm run build && electron-builder --win",
    "package:mac": "npm run build && electron-builder --mac",
    "package:linux": "npm run build && electron-builder --linux",
    "package:all": "npm run build && electron-builder --win --mac --linux",
    "package:windows": "npm run build && electron-builder --win",
    "dist": "node scripts/build.js",
    "dist:win": "node scripts/build.js windows",
    "dist:mac": "node scripts/build.js mac",
    "dist:linux": "node scripts/build.js linux",
    "verify:build": "node scripts/verify-build-config.js",
    "generate:checksums": "node scripts/generate-checksums.js",
    "prepare:release": "pwsh -File scripts/prepare-release.ps1",
    "prepare:release:dry": "pwsh -File scripts/prepare-release.ps1 -DryRun",
    "release": "pwsh -File scripts/release-to-github.ps1",
    "release:dry": "pwsh -File scripts/release-to-github.ps1 -DryRun",
    "verify:release": "node scripts/verify-final-release.js",
    "verify:release:skip-downloads": "node scripts/verify-final-release.js --skip-downloads",
    "ci": "npm run lint && npm run type-check && npm run test:run && npm run build",
    "ci:test": "npm run test:run",
    "ci:build": "npm run build"
  },
  "keywords": [
    "peft",
    "fine-tuning",
    "machine-learning",
    "lora",
    "qlora",
    "transformers",
    "huggingface",
    "electron",
    "react",
    "pytorch",
    "desktop-app",
    "ai",
    "deep-learning"
  ],
  "author": "PEFT Studio Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ankesh-007/peft-studio.git"
  },
  "homepage": "https://github.com/Ankesh-007/peft-studio#readme",
  "bugs": {
    "url": "https://github.com/Ankesh-007/peft-studio/issues"
  },
  "dependencies": {
    "@types/react-window": "^1.8.8",
    "clsx": "^2.1.1",
    "electron-log": "^5.2.2",
    "electron-updater": "^6.6.2",
    "lucide-react": "^0.555.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-window": "^2.2.3",
    "recharts": "^3.5.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "electron": "^39.2.4",
    "electron-builder": "^25.1.8",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "fast-check": "^3.15.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^23.2.0",
    "playwright": "^1.57.0",
    "postcss": "^8.4.49",
    "prettier": "^3.7.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.15",
    "terser": "^5.44.1",
    "typescript": "^5.7.2",
    "vite": "^7.2.6",
    "vitest": "^4.0.14",
    "yaml": "^2.8.2"
  },
  "build": {
    "appId": "com.peftstudio.app",
    "productName": "PEFT Studio",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "backend/**/*",
      "package.json"
    ],
    "publish": {
      "provider": "github",
      "owner": "Ankesh-007",
      "repo": "peft-studio"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "publisherName": "PEFT Studio",
      "verifyUpdateCodeSignature": false,
      "sign": "./scripts/sign-windows.js",
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "fileAssociations": [
        {
          "ext": "peft",
          "name": "PEFT Configuration",
          "description": "PEFT Studio Configuration File"
        }
      ]
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "category": "public.app-category.developer-tools",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "notarize": {
        "teamId": "${APPLE_TEAM_ID}"
      },
      "identity": null,
      "type": "distribution",
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "minimumSystemVersion": "10.13.0",
      "darkModeSupport": true,
      "fileAssociations": [
        {
          "ext": "peft",
          "name": "PEFT Configuration",
          "description": "PEFT Studio Configuration File",
          "role": "Editor"
        }
      ]
    },
    "dmg": {
      "title": "${productName} ${version}",
      "contents": [
        {
          "x": 130,
          "y": 220,
          "type": "file"
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "window": {
        "width": 540,
        "height": 380
      },
      "iconSize": 128,
      "iconTextSize": 12,
      "format": "ULFO",
      "artifactName": "${productName}-${version}-${arch}.${ext}"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "Development",
      "maintainer": "PEFT Studio Contributors <peft-studio@example.com>",
      "vendor": "PEFT Studio",
      "synopsis": "Parameter-Efficient Fine-Tuning Studio",
      "description": "Desktop application for Parameter-Efficient Fine-Tuning of Large Language Models",
      "desktop": {
        "Name": "PEFT Studio",
        "Comment": "Parameter-Efficient Fine-Tuning of Large Language Models",
        "GenericName": "Machine Learning Tool",
        "Keywords": "peft;fine-tuning;machine-learning;ai;deep-learning;",
        "Categories": "Development;Science;",
        "StartupNotify": "true",
        "Terminal": "false",
        "Type": "Application"
      },
      "fileAssociations": [
        {
          "ext": "peft",
          "name": "PEFT Configuration",
          "description": "PEFT Studio Configuration File",
          "mimeType": "application/x-peft-config"
        }
      ],
      "artifactName": "${productName}-${version}-${arch}.${ext}"
    },
    "appImage": {
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "license": "LICENSE"
    },
    "deb": {
      "depends": [
        "gconf2",
        "gconf-service",
        "libnotify4",
        "libappindicator1",
        "libxtst6",
        "libnss3"
      ],
      "priority": "optional",
      "artifactName": "${productName}-${version}-${arch}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "PEFT Studio",
      "deleteAppDataOnUninstall": false,
      "runAfterFinish": true,
      "menuCategory": true,
      "displayLanguageSelector": false,
      "perMachine": false,
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "portable": {
      "artifactName": "${productName}-Portable-${version}.${ext}"
    }
  }
}